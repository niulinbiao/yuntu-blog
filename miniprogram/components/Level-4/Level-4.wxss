/* components/Level-4/Level-4.wxss */
/* 颜色组 */
.color {
  --bgd-color: #cbe9f4;
  --light: #93d5eb;
  --medium: #66bbd8;
  --dark: #4da2bb;
  --bush: #fff;
  --trunk: #c2653c;
  --trunk2: #9d5d5d;
  --cloud: #fff;
  --sun: transparent;
  --rabbit: #fff;
}

/* 动画 */
.container-self view, .container-self:after, .container-self:before {
  transition: 4s ease background;
  position: absolute;
}

/* 外壳 */
.out {
  padding: 0;
  margin: 0 22rpx 0 22rpx;
  overflow: hidden;
  background-color: var(--bgd-color);
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
  /* border-radius: 30rpx; */
  /* height: 100vh; */
  /* height: 450rpx; */
  /* width: 100vw; */
  transition: 4s ease background;
}

/* 内容 */

.container-self {
  position: relative;
  /* top: 70rpx; */
  width: 800rpx;
  height: 450rpx;
}

/* 太阳 */

.sun {
  background-color: var(--sun);
  width: 60rpx;
  height: 60rpx;
  border-radius: 100%;
  top: -20rpx;
  left: 180rpx;
}

/* 彩虹组 */

.rainbow {
  visibility: hidden;
  width: 600rpx;
  height: 270rpx;
  bottom: 82rpx;
  left: 50%;
  transform: translateX(-50%);
}

.rainbow.animated {
  animation: rainbow 14s linear;
  visibility: visible;
}

.rainbow .rainbow-colour {
  border-top-left-radius: 50% 100%;
  border-top-right-radius: 50% 100%;
  left: 50%;
  transform: translate(-50%, 0);
  transform-origin: 0 100%;
  bottom: 0;
  border-bottom: 0 !important;
}

.rainbow .rainbow-colour:nth-child(1) {
  border: 10rpx solid #ac01fd;
  height: 100%;
  width: 100%;
}

.rainbow .rainbow-colour:nth-child(2) {
  border: 10rpx solid #511ce0;
  height: 261rpx;
  width: 581rpx;
}

.rainbow .rainbow-colour:nth-child(3) {
  border: 10rpx solid #1a9fe2;
  height: 252rpx;
  width: 562rpx;
}

.rainbow .rainbow-colour:nth-child(4) {
  border: 10rpx solid #5ed81d;
  height: 243rpx;
  width: 543rpx;
}

.rainbow .rainbow-colour:nth-child(5) {
  border: 10rpx solid #f8f52a;
  height: 234rpx;
  width: 524rpx;
}

.rainbow .rainbow-colour:nth-child(6) {
  border: 10rpx solid #f7a528;
  height: 225rpx;
  width: 505rpx;
}

.rainbow .rainbow-colour:nth-child(7) {
  border: 10rpx solid #f63b2a;
  height: 216rpx;
  width: 486rpx;
}

@keyframes rainbow {
  0% {
      opacity: 0;
  }

  10% {
      opacity: 0.5;
  }

  80% {
      opacity: 0.5;
  }

  100% {
      opacity: 0;
  }
}

/* 云组 */

.cloud-group {
  width: 100%;
  height: 100%;
  top: 0;
  left: 0;
  overflow: hidden;
}

.cloud {
  background-color: var(--cloud);
  width: 80rpx;
  height: 30rpx;
  border-radius: 50rpx;
  left: 50rpx;
  top: 40rpx;
  animation: clouds 10s linear infinite;
}

@keyframes clouds {
  50% {
