  height: 12rpx;
  background-color: #ff8cd0;
  border-radius: 50%;
  top: 0;
  left: -100%;
  transform-origin: 50% 50%;
}

.flower .petal:nth-child(2) {
  transform: rotate(55deg);
}

.flower .petal:nth-child(3) {
  transform: rotate(120deg);
}

.flower:nth-child(6) {
  top: -70rpx;
  left: -22rpx;
}

@keyframes flower {
  0%, 100% {
      transform: scale(0);
  }

  20%, 80% {
      transform: scale(0.8);
  }
}

.rabbit {
  visibility: hidden;
  left: 278rpx;
  top: -10rpx;
  width: 20rpx;
  height: 50rpx;
}

.rabbit.animated {
  animation: rabbit 6s ease 2;
  visibility: visible;
}

.rabbit .head {
  width: 20rpx;
  height: 22rpx;
  background-color: var(--rabbit);
  border-radius: 50%;
}

.rabbit .head:after {
  content: "";
  top: 8rpx;
  left: 5rpx;
  background-color: #000;
  border-radius: 50%;
  height: 3rpx;
  width: 3rpx;
  box-shadow: 8rpx 0 #000;
}

.rabbit .head:before {
  content: "";
  top: 13rpx;
  left: 9rpx;
  background-color: #ff8cd0;
  border-radius: 50%;
  height: 3rpx;
  width: 3rpx;
}

.rabbit .ear {
  left: 2rpx;
  top: -16rpx;
  height: 16rpx;
  width: 3rpx;
  background-color: #ff8cd0;
  border: var(--rabbit) solid 2rpx;
  border-radius: 50% 50% 0 0;
  transform: rotate(-5deg);
}

.rabbit .ear:nth-child(2) {
  left: auto;
  right: 2rpx;
  transform: rotate(5deg);
}

@keyframes rabbit {
  0%, 100%, 80% {
      transform: translateY(0);
  }

  40%, 60% {
      transform: translateY(-25rpx);
  }
}


.btn-style{
  transition: 4s ease all;
  background-color: var(--light);
  color:var(--bush);
}
